<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="ibmMQCfg" class="dcsiclear.thread.IBMMQCfg">
		<property name="queueManager">
			<value>QM_APPLE</value>
			<!-- <value>QM_AMB</value> -->
		</property>
		<property name="ccsid">
			<value>1381</value>
		</property>
		<property name="channel">
			<value>DC.SVRCONN</value>
		</property>
		<property name="hostName">
			<value>127.0.0.1</value>
			<!-- <value>127.0.0.1</value> -->
		</property>
		<property name="port">
			<value>1415</value>
			<!-- <value>1417</value> -->
		</property>
		<property name="queueName">
			<value>Q1</value>
		</property>
		<property name="clientId">
			<value>MUSR_MQADMIN</value>
		</property>
		<property name="targetClient">
			<value>1</value>
		</property>
		<property name="encoding">
			<value>546</value>
		</property>
		<property name="maxdepth">
			<value>1</value>
		</property>
		<property name="charsetEncoding">
			<value>UTF-16LE</value>
		</property>
	</bean>
</beans>